{
	"name": "Malayalam Blog Reader",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Get latest updates from malayalam blogs in chrome toolbar.",
	"icons": {
		"128": "images/icon_128.png",
		"48": "images/icon_48.png",
		"32": "images/icon_32.png",
		"24": "images/icon_24.png",
		"19": "images/icon_19.png",
		"16": "images/icon_16.png"
	},
	"browser_action": {
		"default_icon": "images/icon_19.png",
		"default_popup": "popup.html",
		"default_title": "Malayalam Blog Reader"
	},
	"background": {
		"scripts": [ "js/prefs.js", "js/feed.js", "js/feedreader.js", "js/timer.js", "js/background.js" ]
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"options_page": "options.html",
	"permissions": ["http://*/*", "https://*/*"],
	"update_url": "http://mal-blogreader.sourceforge.net/updates.xml"
}
